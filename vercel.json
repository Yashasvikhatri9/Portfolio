{
  "version": 2,
  "builds": [
    {
      "src": "portfolio/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "portfolio/build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/portfolio/build/$1" }
  ],
  "env": {
    "REACT_APP_EXAMPLE_NAME": "I9JU23NF394R6HH"
    // Add more environment variables here as needed
  }
}
